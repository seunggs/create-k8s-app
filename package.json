{
  "name": "@sidetrek/create-k8s-app",
  "version": "0.0.49",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "main": "bin/index.js",
  "bin": {
    "cka": "bin/index.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "cka:init": "cka init",
    "start:init": "npm run build && npm run cka:init",
    "cka:app": "cka app",
    "start:app": "npm run build && npm run cka:app",
    "cka:destroy": "cka destroy",
    "destroy": "npm run build && npm run cka:destroy",
    "server:prod": "npx nodemon server/server.js",
    "release": "npm run build && np",
    "test": "echo \"No test specified\""
  },
  "dependencies": {
    "@pulumi/aws": "^4.0.0",
    "@pulumi/awsx": "^0.30.0",
    "@pulumi/eks": "^0.35.0",
    "@pulumi/kubernetes": "^3.12.1",
    "@pulumi/kubernetes-cert-manager": "^0.0.1",
    "@pulumi/pulumi": "^3.21.0",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.5",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "dotenv": "^10.0.0",
    "enquirer": "^2.3.6",
    "express": "^4.17.2",
    "fs-extra": "^10.0.0",
    "gradient-string": "^1.2.0",
    "js-yaml": "^3.14.1",
    "log-update": "^4.0.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^14.17.33",
    "nodemon": "^2.0.15",
    "ts-node": "^7.0.1",
    "typescript": "^3.7.7"
  }
}
